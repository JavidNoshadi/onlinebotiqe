<div class="panel">
    <h1>{{msg}}</h1>
    <h3>{{succ_msg}}</h3>
    <a href="logout">logout</a>
    <a href='/dashboard/new'>new</a>
</div>
{{#if stories}}
        {{#each stories}}
       <div class="dashboard-card">
           <h4>{{product_name}}</h4>
            <img src="http://localhost:7700/{{file.[0]}}"/>
            <h4>{{quantity}}</h4>
            <h6>{{price}}</h6>
            
            <a href="/dashboard/edit/{{_id}}">نمایش</a>
            <a href='/dashboard/{{_id}}'>ویرایش</a>
       </div>
        {{/each}}
    

{{else}}
<p>You have not created any stories</p>
{{/if}}